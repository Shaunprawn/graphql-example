{
  "name": "graphql-example",
  "version": "1.1.0",
  "description": "A GraphQL application example (intuitive rootValue resolvers using Contextable.js)",
  "main": "./dist/index.js",
  "config": {
    "env": "development",
    "httpHost": "127.0.0.1",
    "httpPort": 4444,
    "mongoUrl": "mongodb://localhost:27017/test"
  },
  "scripts": {
    "build": "mkdir -p ./dist; babel ./src --out-dir ./dist --copy-files",
    "clean": "rm -Rf ./dist",
    "compile": "npm run clean; npm run build",
    "exec": "node --harmony ./dist/scripts/graph-exec",
    "print": "node --harmony ./dist/scripts/graph-print",
    "start": "node --harmony ./dist/scripts/graph-start",
    "test": "node --harmony node_modules/.bin/ava"
  },
  "ava": {
    "files": [
      "./tests/*.js",
      "./tests/**/*.js"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "dependencies": {
    "contextable": "2.9.1",
    "express": "4.14.0",
    "express-graphql": "0.6.1",
    "graphql": "0.8.2",
    "mongodb": "2.2.11"
  },
  "devDependencies": {
    "ava": "0.17.0",
    "babel-cli": "6.18.0",
    "babel-preset-es2017-node7": "0.4.1"
  }
}
